<PhysiCell_settings version="1.3.3">
	<domain>
		<x_min>-1000.0</x_min>
		<x_max>1000.0</x_max>
		<y_min>-1000.0</y_min>
		<y_max>1000.0</y_max>
		<z_min>-10.0</z_min>
		<z_max>10.0</z_max>
		<dx>20.0</dx>
		<dy>20.0</dy>
		<dz>20.0</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">64800.0</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>.</folder>  

		<full_data>
			<interval units="min">60</interval>
			<enable>True</enable>
		</full_data>
		 
		<SVG>
			<interval units="min">1</interval>
			<enable>True</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<user_parameters>
		<random_seed type="int" unit="dimensionless">0</random_seed>
		<self_velocity_factor type="double" units="dimless">0.5</self_velocity_factor>
		<neigh_velocity_factor type="double" units="dimless">0.4</neigh_velocity_factor>
		<previous_bias_factor type="double" units="dimless">0.1</previous_bias_factor>
		<noise_factor type="double" units="dimless">0.1</noise_factor>
		
		<motile_cell_persistence_time type="double" units="min">0.05</motile_cell_persistence_time>
		<motile_cell_migration_speed type="double" units="um/min">0.25</motile_cell_migration_speed>
		
		<pressure_threshold type="double" units="dimless">0.4</pressure_threshold>
		
		<relative_adhesion_strength type="double" units="dimless">0.01</relative_adhesion_strength>
		<relative_repulsion_strength type="double" units="dimless">1.25</relative_repulsion_strength>
		
		 
		
	</user_parameters>
	
</PhysiCell_settings>